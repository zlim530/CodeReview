<script>
    function getPromotionRecommendations() {
        $(document).trigger('homepage-promotions-banner-clicked-gtm');
        $.post({
            url: "/recommender/getRecommendedProductsForPromotions",
            data: {productId: 0, impressions: []}
        }).done(function (data) {
            $("#homepage-banner-recommedations").html(data);

            if(!data.includes("img")) {
                $(".customizable-recommendations-container").css("display","none");
            }
        }).fail(function() {
            $(".customizable-recommendations-container").css("display","none");
        });
    }

    function loadPromotions(longerVersion) {
        $.ajax({
            url: "https://dlhv9ioap0i6d.cloudfront.net/promotions" + (longerVersion ? "-long" : "") + ".html",
            crossDomain: true,
            cache: false
        }).done(function( data ) {
            if(data.trim().length > 0) {
                $("#promotions-and-recommendations-modal").css("display", "");
                $("#dynamic-promotions-placeholder").html(data);
                $(document).trigger('homepage-promotions-banner-shown-gtm');
            }
        }).fail(function() {
        });
    }

    // AB TEST code ------------------------------
    // Variants: 0 - no banner
    //           1 - short modal
    //           2 - long modal

    $(document).ready(function() {

        function gtag() {dataLayer.push(arguments)}

        function implementExperimentOnHomepage(value) {
            console.log("Variant", value);

            if (value ==  '1') {
                loadPromotions(false);
            }
            else if (value ==  '2') {
                loadPromotions(true);
            }
            else {
                // original scenario
                console.log("Original scenario");
            }
        }

        gtag('event', 'optimize.callback', {
            name: 'x3nwXgo8SZKOPhJ1UTkQNQ',
            callback: implementExperimentOnHomepage
        });
    });
    // AB TEST code end------------------------------

</script>

                        

